⇇!DOCTYPE⇔gw1⇉⇇META⇔Sun Aug 11 2024 21:37:31 GMT-0600 (Mountain Daylight Time)⇉⇇TITLE⇔Object-Based Programming⇔gw_user⇉⇇JSCONTENT⇔let player = ↴{↴    x:400,↴    y:400,↴    speed:5,↴    move: function(mx, my)↴    {↴        this.x += mx;↴        this.y += my;↴    },↴    render: function()↴    {↴       draw.fillStyle = "green";↴       draw.fillRect(this.x-20, this.y-2, 40, 40);↴    }↴};↴let enemy;↴{↴    let clones = [];↴    enemy = class↴    {↴        constructor(cx, cy, cw, ch, cColour)↴        {↴            this.x = cx;↴            this.y = cy;↴            this.width = cw;↴            this.height = ch;↴            this.colour = cColour;↴            clones.push(this);↴        }↴        move()↴        {↴            this.x += (player.x-this.x)/100;↴            this.y += (player.y-this.y)/100;↴        }↴        render()↴        {↴            draw.fillStyle = this.colour;↴            draw.fillRect↴            (↴            this.x- (this.width/2),↴            this.y-(this.height/2),↴            this.width, ↴            this.height↴            );↴        }↴        loop()↴        {↴            this.move();↴            this.render();↴        }↴    };↴    enemy.runAllLoop = function()↴    {↴        clones.forEach(clone=>clone.loop());↴    };↴}↴↴new enemy(1, 1, 20, 20, "red");↴new enemy(800, 800, 25, 25, "blue");↴new enemy(800, 1, 30, 30, "purple");↴function forever()↴{↴    draw.background("white");↴    if(keyboard.includes("d")) player.move(player.speed, 0);↴    if(keyboard.includes("a")) player.move(-player.speed, 0);↴    if(keyboard.includes("w")) player.move(0, -player.speed);↴    if(keyboard.includes("s")) player.move(0, player.speed);↴    player.render();↴    enemy.runAllLoop();↴}⇉⇇END⇉